{
	"name": "davatar",
	"version": "2.7.3",
	"scripts": {
		"dev": "vite",
		"dev:mock": "vite dev --mode mock",
		"build": "rimraf dist/assets/* && vite build",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
		"bump": "bumpp package.json --commit \"release: v\" --push --tag",
		"prepare": "npx simple-git-hooks"
	},
	"dependencies": {
		"@vueuse/components": "^10.3.0",
		"@vueuse/core": "^10.3.0",
		"axios": "^1.4.0",
		"hoci": "^0.0.12",
		"pinia": "^2.1.6",
		"query-string": "^8.1.0",
		"tslx": "^0.1.0",
		"vue": "^3.3.4",
		"vue-router": "^4.2.4",
		"vue-virtual-scroller": "2.0.0-beta.8",
		"workbox-expiration": "^7.0.0",
		"workbox-strategies": "^7.0.0"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,vue}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{css,less,scss}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@curev/eslint-config": "^0.2.1",
		"@fourze/core": "^0.28.3",
		"@fourze/vite": "^0.28.3",
		"@iconify-json/ic": "^1.1.13",
		"@types/node": "^20.4.8",
		"@unocss/preset-icons": "^0.54.1",
		"@unocss/preset-mini": "^0.54.1",
		"@unocss/preset-uno": "^0.54.1",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitejs/plugin-vue-jsx": "^3.0.1",
		"bumpp": "^9.1.1",
		"eslint": "^8.46.0",
		"less": "^4.2.0",
		"lint-staged": "^14.0.1",
		"prettier": "^3.0.1",
		"rimraf": "^5.0.1",
		"sass": "^1.64.2",
		"simple-git-hooks": "^2.9.0",
		"typescript": "^5.1.6",
		"unocss": "^0.54.1",
		"unocss-preset-palette": "^0.2.1",
		"unplugin-vue-components": "^0.25.1",
		"vite": "^4.4.8",
		"vite-plugin-pwa": "^0.16.4",
		"workbox-core": "^7.0.0",
		"workbox-precaching": "^7.0.0",
		"workbox-routing": "^7.0.0",
		"workbox-window": "^7.0.0"
	}
}
