{
	"name": "davatar",
	"version": "2.5.0-beta.1",
	"scripts": {
		"dev": "vite",
		"dev:mock": "vite dev --mode mock",
		"build": "rimraf dist/assets/* && vite build",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
		"bump": "bumpp package.json --commit \"release: v\" --push --tag",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@vueuse/core": "^9.12.0",
		"axios": "^0.27.2",
		"hoci": "^0.0.6",
		"pinia": "^2.0.14",
		"query-string": "^7.1.1",
		"vue": "^3.2.47",
		"vue-router": "^4.1.5"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,vue}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{css,less,scss}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@curev/eslint-config": "^0.0.7",
		"@fourze/core": "^0.24.4",
		"@fourze/vite": "^0.24.4",
		"@iconify-json/ic": "^1.1.3",
		"@types/node": "^17.0.31",
		"@unocss/preset-icons": "^0.45.26",
		"@unocss/preset-mini": "^0.45.26",
		"@unocss/preset-uno": "^0.45.26",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"bumpp": "^7.1.1",
		"eslint": "^8.27.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"sass": "^1.51.0",
		"typescript": "^4.9.5",
		"unocss": "^0.49.4",
		"unocss-preset-palette": "^0.0.5",
		"unplugin-vue-components": "^0.23.0",
		"vite": "^4.1.1",
		"vite-plugin-pwa": "^0.14.3",
		"workbox-core": "^6.5.4",
		"workbox-precaching": "^6.5.4",
		"workbox-routing": "^6.5.4",
		"workbox-window": "^6.5.4"
	}
}
