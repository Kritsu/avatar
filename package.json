{
	"name": "davatar",
	"version": "2.6.10",
	"scripts": {
		"dev": "vite",
		"dev:mock": "vite dev --mode mock",
		"build": "rimraf dist/assets/* && vite build",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
		"bump": "bumpp package.json --commit \"release: v\" --push --tag",
		"prepare": "npx simple-git-hooks"
	},
	"dependencies": {
		"@vueuse/components": "^9.13.0",
		"@vueuse/core": "^9.13.0",
		"axios": "^1.4.0",
		"hoci": "^0.0.10",
		"pinia": "^2.1.3",
		"query-string": "^7.1.3",
		"vue": "^3.3.4",
		"vue-router": "^4.2.2",
		"vue-virtual-scroller": "2.0.0-beta.8",
		"workbox-expiration": "^6.6.0",
		"workbox-strategies": "^6.6.0",
    "tslx": "^0.0.9"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,vue}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{css,less,scss}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@curev/eslint-config": "^0.0.11",
		"@fourze/core": "^0.27.12",
		"@fourze/vite": "^0.27.12",
		"@iconify-json/ic": "^1.1.13",
		"@types/node": "^17.0.31",
		"@unocss/preset-icons": "^0.53.1",
		"@unocss/preset-mini": "^0.53.1",
		"@unocss/preset-uno": "^0.53.1",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitejs/plugin-vue-jsx": "^3.0.1",
		"bumpp": "^7.2.0",
		"eslint": "^8.42.0",
		"less": "^4.1.3",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"rimraf": "^3.0.2",
		"sass": "^1.63.4",
		"simple-git-hooks": "^2.8.1",
		"typescript": "^5.1.3",
		"unocss": "^0.53.1",
		"unocss-preset-palette": "^0.0.5",
		"unplugin-vue-components": "^0.25.1",
		"vite": "^4.3.9",
		"vite-plugin-pwa": "^0.16.4",
		"workbox-core": "^6.6.0",
		"workbox-precaching": "^6.6.0",
		"workbox-routing": "^6.6.0",
		"workbox-window": "^6.6.0"
	}
}
