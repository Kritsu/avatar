{
	"name": "davatar",
	"version": "2.2.1",
	"scripts": {
		"dev": "vite",
		"dev:mock": "vite --mode production",
		"build": "rimraf dist/assets/* && vite build",
		"serve": "vite preview",
		"release": "bumpp package.json --commit \"release: v\" --push --tag",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@vueuse/core": "^8.9.4",
		"axios": "^0.27.2",
		"pinia": "^2.0.14",
		"query-string": "^7.1.1",
		"vue": "^3.2.33",
		"vue-router": "^4.0.15"
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": [
			"prettier --write"
		],
		"*.vue": [
			"prettier --write"
		],
		"*.{less,s?css}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@fourze/core": "^0.10.0",
		"@fourze/vite": "^0.10.0",
		"@iconify-json/ic": "^1.1.3",
		"@types/node": "^17.0.31",
		"@unocss/preset-icons": "^0.44.3",
		"@unocss/preset-mini": "^0.44.3",
		"@unocss/preset-uno": "^0.44.3",
		"@vitejs/plugin-vue": "^3.0.3",
		"@vitejs/plugin-vue-jsx": "^2.0.1",
		"bumpp": "^7.1.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"node-fetch": "^3.2.10",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"sass": "^1.51.0",
		"typescript": "^4.7.4",
		"unocss": "^0.44.3",
		"unocss-preset-palette": "^0.0.5",
		"unplugin-vue-components": "^0.21.1",
		"vite": "^3.0.9",
		"vite-plugin-pwa": "^0.12.3",
		"workbox-core": "^6.5.3",
		"workbox-precaching": "^6.5.3",
		"workbox-routing": "^6.5.3",
		"workbox-window": "^6.5.3"
	}
}
